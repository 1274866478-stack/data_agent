{
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--project-from-cwd",
        "--context",
        "desktop-app",
        "--mode",
        "interactive,editing",
        "--log-level",
        "INFO"
      ]
    },
    "chrome-devtools": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "chrome-devtools-mcp"
      ]
    },
    "context7": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "context7"
      ]
    },
    "exa": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@extralife/mcp-server-exa"
      ]
    },
    "ide": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-ide"
      ]
    },
    "deepwiki": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "mcp-deepwiki"
      ]
    },
    "open-websearch": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@mcp-im/open-websearch"
      ]
    }
  }
}